ams
arn
atl
bog - 2019-01-06

# Run with
for d in ams arn atl bog ; do
  ./makeMeasurement -apiKey api-keys -count 10 -md 96h -measurement measurements/gdns_v4.json -metro ${d} -v4
  ./makeMeasurement -apiKey api-keys -count 10 -md 96h -measurement measurements/gdns_v6.json -metro ${d} -v6
  ./makeMeasurement -apiKey api-keys -count 10 -md 96h -measurement measurements/gdns_v4_norecurse.json -metro ${d} -v4
  ./makeMeasurement -apiKey api-keys -count 10 -md 96h -measurement measurements/gdns_v6_norecurse.json -metro ${d} -v6
done
